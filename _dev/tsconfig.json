{
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "declaration": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "nodenext",
    "outDir": "dist",
    "paths": {
      "svelte": ["./node_modules/svelte/types/runtime/index.d.ts"],
      "svelte/motion": ["./node_modules/svelte/types/runtime/motion/index.d.ts"],
      "svelte/store": ["./node_modules/svelte/types/runtime/store/index.d.ts"]
    },
    "target": "es2020",
  },
  "include": [
    "./src/**/*.ts",
    "./src/**/*.svelte",
    "./node_modules/@sveadmin/**/*.ts",
    "./node_modules/@sveadmin/**/*.svelte",
    "./node_modules/@sveadmin/table/node_modules/@sveadmin/**/*.ts",
    "./node_modules/@sveadmin/table/node_modules/@sveadmin/**/*.svelte",
    "./node_modules/@sveadmin/table/node_modules/@sveadmin/element/node_modules/@sveadmin/**/*.ts",
    "./node_modules/@sveadmin/table/node_modules/@sveadmin/element/node_modules/@sveadmin/**/*.svelte",
    "./node_modules/@sveadmin/table/node_modules/@sveadmin/common/node_modules/@sveadmin/**/*.ts",
    "./node_modules/@sveadmin/table/node_modules/@sveadmin/common/node_modules/@sveadmin/**/*.svelte",
    "../common/src/main.d.ts",
    "../element/src/main.d.ts",
    "../table/src/main.d.ts"
  ],
  "typeRoots": [
    "./node_modules/@types",
    "./node_modules/svelte/**/*.d.ts",
    "./node_modules/@sveadmin/**/*.d.ts"
  ]
}